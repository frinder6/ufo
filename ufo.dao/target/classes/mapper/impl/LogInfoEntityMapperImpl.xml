<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ufo.mapper.impl.LogInfoEntityMapperImpl">
    <resultMap id="BaseResultMap" extends="com.ufo.mapper.LogInfoEntityMapper.ResultMapWithBLOBs"
               type="com.ufo.entity.LogInfoEntityWithBLOBs">
    </resultMap>
    <sql id="Base_Column_List">
        <include refid="com.ufo.mapper.LogInfoEntityMapper.Base_Column_List"/>
        ,
        <include refid="com.ufo.mapper.LogInfoEntityMapper.Blob_Column_List"/>
    </sql>

    <sql id="Base_Where_Clause">
        <if test="e.host != null">
            AND `host` LIKE #{e.host,jdbcType=VARCHAR}"%"
        </if>
        <if test="e.handler != null">
            AND `handler` LIKE #{e.handler,jdbcType=VARCHAR}"%"
        </if>
        <if test="e.requestUrl != null">
            AND request_url LIKE #{e.requestUrl,jdbcType=VARCHAR}"%"
        </if>
        <if test="e.requestParams != null">
            AND request_params LIKE #{e.requestParams,jdbcType=VARCHAR}"%"
        </if>
        <if test="e.className != null">
            AND class_name LIKE #{e.className,jdbcType=VARCHAR}"%"
        </if>
        <if test="e.method != null">
            AND `method` LIKE #{e.method,jdbcType=VARCHAR}"%"
        </if>
        <if test="e.status != null">
            AND `status` = #{e.status,jdbcType=BIT}
        </if>
        <if test="e.createTime != null">
            AND create_time = #{e.createTime,jdbcType=TIMESTAMP}
        </if>
        <if test="e.creater != null">
            AND creater = #{e.creater,jdbcType=VARCHAR}
        </if>
        <if test="e.updateTime != null">
            AND update_time = #{e.updateTime,jdbcType=TIMESTAMP}
        </if>
        <if test="e.updater != null">
            AND updater = #{e.updater,jdbcType=VARCHAR}
        </if>
    </sql>
    <!-- 分页查询 -->
    <select id="selectPage" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM ufo_log_info
        WHERE 1 = 1
        <include refid="Base_Where_Clause"/>
        ORDER BY ${p.sort} ${p.order}
        LIMIT #{p.pageFrom}, #{p.pageSize}
    </select>
    <select id="selectPageCount" resultType="java.lang.Integer">
        SELECT
        COUNT(1)
        FROM ufo_log_info
        WHERE 1 = 1
        <include refid="Base_Where_Clause"/>
    </select>
</mapper>