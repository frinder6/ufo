<mapper namespace="com.ufo.mapper.impl.DictInfoEntityMapperImpl">
    <resultMap id="BaseResultMap" extends="com.ufo.mapper.DictInfoEntityMapper.BaseResultMap"
               type="com.ufo.entity.DictInfoEntity">
    </resultMap>
    <sql id="Base_Column_List">
        <include refid="com.ufo.mapper.DictInfoEntityMapper.Base_Column_List"/>
    </sql>
